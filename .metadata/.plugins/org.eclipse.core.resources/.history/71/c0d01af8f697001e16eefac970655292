#ifndef CANCOM_H_
#define CANCOM_H_

#define SENSOR_NODE			1

#if SENSOR_NODE
#define TX_ID				ACTUATOR_ID
#define	RX_ID				SENSOR_ID
#elif	ACTUATOR_NODE
#define TX_ID				SENSOR_ID
#define	RX_ID				ACTUATOR_ID
#endif

void CANCom_Init();


#endif /* CANCOM_H_ */
