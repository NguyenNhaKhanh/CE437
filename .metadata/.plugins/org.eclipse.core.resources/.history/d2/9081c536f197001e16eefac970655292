//		CAN_TxHeaderTypeDef TxHeader = { .DLC = 8, .IDE = CAN_ID_STD, .RTR =
//				CAN_RTR_DATA, .StdId = 0x27C, };

	/* USER CODE END 2 */

	/* Infinite loop */
	/* USER CODE BEGIN WHILE */
	while (1) {

//		status = HAL_CAN_AddTxMessage(&hcan, &TxHeader, Data1, &MailBox);
//				if (status != HAL_OK) {
//					HAL_UART_Transmit(&huart1, Transmit_Fail, sizeof(Transmit_Fail), 100);
